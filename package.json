{
  "name": "smart-video-processor",
  "private": true,
  "version": "1.4.8",
  "description": "Automated video processing. Re-Organize, Re-encode, adds metadata and image.",
  "main": "./build/main/index.js",
  "author": "Xavier Fuentes",
  "license": "GPL-3.0+",
  "type": "module",
  "bugs": {
    "url": "https://github.com/xfuentes/smart-video-processor/issues"
  },
  "homepage": "https://github.com/xfuentes/smart-video-processor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xfuentes/smart-video-processor.git"
  },
  "keywords": [
    "video",
    "processing",
    "encode"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "fstart": "electron-forge start",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "package": "electron-vite build && electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-vite build && electron-forge publish",
    "prepare-snap": "node ./tools/configure.js ./snap/ && node ./tools/configure.js ./snap/gui",
    "test": "vitest run",
    "coverage": "vitest --coverage.enabled"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "electron-updater": "^6.3.9",
    "update-electron-app": "^3.1.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-snap": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/publisher-github": "^7.9.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@fluentui/react": "^8.123.4",
    "@fluentui/react-components": "^9.69.0",
    "@fluentui/react-icons": "^2.0.308",
    "@fluentui/react-timepicker-compat":  "^0.4.20",
    "@inquirer/prompts": "^7.7.1",
    "@reforged/maker-appimage": "^5.0.0",
    "@reforged/maker-types": "^1.0.1",
    "@types/cli-color": "0.3.29",
    "@types/cli-progress": "^3.11.6",
    "@types/clui": "^0.3.4",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.16.5",
    "@types/react": "18.3.19",
    "@types/react-dom": "^18.3.5",
    "@types/tmp": "^0.2.6",
    "@types/yargs": "^17.0.33",
    "@uifabric/icons": "^7.9.6",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "axios": "^1.11.0",
    "cli-color": "0.3.2",
    "cli-progress": "^3.12.0",
    "clui": "^0.3.6",
    "electron": "^37.2.3",
    "electron-builder": "^25.1.8",
    "electron-squirrel-startup": "^1.0.1",
    "electron-vite": "^4.0.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "glob": "^11.0.3",
    "lcid": "^5.0.0",
    "lodash": "^4.17.21",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tmp": "^0.2.5",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "vite": "^7.1.5",
    "vitest": "^3.0.9",
    "yargs": "^18.0.0"
  },
  "optionalDependencies": {
    "ntsuspend": "^1.0.2"
  },
  "config": {
    "forge": "./forge.config.ts"
  }
}
